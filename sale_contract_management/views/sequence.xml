<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Sequences for sale Contract -->
        <record id="seq_sale_contract" model="ir.sequence">
            <field name="name">Sale Contract</field>
            <field name="code">sale.contract</field>
            <field name="prefix">Contract/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <!-- Sequences for sale Contract Line-->
        <record id="seq_sale_contract_line" model="ir.sequence">
            <field name="name">Sale Contract Line</field>
            <field name="code">sale.contract.line</field>
            <field name="prefix">Ship/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>