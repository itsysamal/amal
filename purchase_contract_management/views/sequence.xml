<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Sequences for Purchase Contract -->
        <record id="seq_purchase_contract" model="ir.sequence">
            <field name="name">Purchase Contract</field>
            <field name="code">purchase.contract</field>
            <field name="prefix">Contract/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <!-- Sequences for Purchase Contract Line-->
        <record id="seq_purchase_contract_line" model="ir.sequence">
            <field name="name">Purchase Contract Line</field>
            <field name="code">purchase.contract.line</field>
            <field name="prefix">Ship/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>